<div class="admin-content">
    <div class="am-cf am-padding">
        <div class="am-fl am-cf"><strong class="am-text-primary am-text-lg">营销短信</strong> /
            <small>发送结果</small>
        </div>
    </div>

    <div class="am-g">
        <div class="am-u-sm-12 am-u-sm-centere am-text-left">
            <div class="am-u-sm-12 am-u-md-2 am-text-right admin-form-text">

            </div>
            <div class="am-u-sm-12 am-u-md-10 ">
            #if($successMsg)
                <span class="am-text-success am-text-xl">发送成功</span>
                <span class="am-text-success am-text-xl">$!successMsg</span>
            #end

            #if($errorMsg)
                <span class="am-text-danger am-text-xl">发送失败</span>
                <span class="am-text-danger">原因:$!errorMsg</span>
            #end
           </div>
        </div>

    </div>
</div>